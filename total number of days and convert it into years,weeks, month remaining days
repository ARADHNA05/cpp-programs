//Q8.input total number of days and convert it into years,weeks, month remaining days
#include <iostream>
using namespace std;
int main(){
    int day, week, month, year;
    
    cout<<"ENTER THE DAY: ";
    cin>>day;
    //logic 
    year=day/365;
    day=day%365;
    
    month=day/30;
    day=day%30;
    
    week=day/7;
    day=day%7;
    
    cout<<"The output according to the given input is: "<<year<<" year, "<<month<<" month,"<<week<<" week,";
    return 0;
}
